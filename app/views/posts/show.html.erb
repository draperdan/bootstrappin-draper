<div class="container">
<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Content:</strong>
  <%= @post.content.html_safe %>
</p>

<%= link_to 'Edit', edit_post_path(@post), class: "btn btn-default", data: { turbolinks: false } %> |
<%= link_to 'Back', posts_path, class: "btn btn-default" %>
</div>